"xbacklight -inc 10"
    XF86MonBrightnessUp
"xbacklight -dec 10"
    XF86MonBrightnessDown
"amixer -c 0 set Master 5+"
    XF86AudioRaiseVolume
"amixer -c 0 set Master 5-"
    XF86AudioLowerVolume
"amixer -c 0 set Master 0"
    XF86AudioMute
"terminator"
    Control + F12
"tilda"
    F12
"wmctrl -s $(expr 1 - `wmctrl -d | grep "*" | cut -d " " -f1`)"
    Control + Alt + w
